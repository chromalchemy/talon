
^((who | will) she | wooshy | wish he) [(hunt | point | search)] [<user.text>] : 
    key(cmd-shift-space)
    sleep(300ms) 
    insert("{text or ''}")

^((who | will) she | wooshy | wish he be) click [<user.text>] : 
    key(cmd-shift-space)
    sleep(300ms) 
    insert("{text or ''}")
    sleep(300ms)
    key(enter)


